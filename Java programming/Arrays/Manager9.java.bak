public class Manager9 
{
	public static void main(String[] args) 
	{
		int[][] x = new int [3][2];
		x[0][0]= 1;
		x[0][1]= 2;
		x[1][0]= 3;
		x[1][1]= 4;
		x[2][0]= 5;
		x[2][1]= 6;
	for (int i =0;i< x.length;i++)
	{
		for (int j=0; j < x[i].length; j++)
//in order to read inner array element we required inner for loop
		{
			System.out.println(x[i][j]);
		}

	}
	
    }
}
